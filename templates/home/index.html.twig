{% extends 'base.html.twig' %}

{# Primary Meta Tags #}
{% block title %}Accueil{% endblock %}
{% block meta_title %}Accueil{% endblock %}
{% block meta_description %}{{ website.description }}{% endblock %}
{% block meta_canonical %}{{ website.domainName }}{% endblock %}
{# <!-- Open Graph / Facebook --> #}
{% block meta_fb_type %}website{% endblock %}
{% block meta_fb_url %}{{ website.domainName }}{% endblock %}
{% block meta_fb_title %}}{{ website.name }}{% endblock %}
{% block meta_fb_description %}{{ website.description }}{% endblock %}
{% block meta_fb_image %}{% endblock %}

{# <!-- Twitter --> #}
{% block meta_twitter_type %}summary_large_image{% endblock %}
{% block meta_twitter_url %}{{ website.domainName }}{% endblock %}
{% block meta_twitter_title %}{% endblock %}
{% block meta_twitter_description %}{{ website.domainName }}{% endblock %}
{% block meta_twitter_image %}{% endblock %}

{% block body_id 'homepage' %}

{#
    the homepage is a special page which displays neither a header nor a footer.
    this is done with the 'trick' of defining empty Twig blocks without any content
#}
{#{% block header %}{% endblock %}#}
